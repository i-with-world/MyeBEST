    163          ************************************************************************************************/
    164          void Wait_Kinetis(void)
    165          {
    166            /* 
    167             * Clear the SLEEPDEEP bit to make sure we go into WAIT (sleep) mode instead
    168             * of deep sleep.
    169             */
    170            SCB_SCR &= ~SCB_SCR_SLEEPDEEP_MASK;	
    171          
    172            /* WFI instruction will start entry into WAIT mode */
    173            asm("WFI");
    174          }
