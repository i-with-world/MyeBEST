    149          void Stop_Kinetis(void)
    150          {
    151            /* Set the SLEEPDEEP bit to enable deep sleep mode (STOP) */
    152            SCB_SCR |= SCB_SCR_SLEEPDEEP_MASK;	
    153          
    154            /* WFI instruction will start entry into STOP mode */
    155            asm("WFI");
    156          }
